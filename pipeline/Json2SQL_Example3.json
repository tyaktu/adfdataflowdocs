{
	"name": "Json2SQL_Example3",
	"properties": {
		"description": "ADF içinde oluşturduğum pipieline. ",
		"activities": [
			{
				"name": "DataLakeJsonExample3toPoolSQLTable",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"writeBehavior": "Insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['color']"
								},
								"sink": {
									"name": "color"
								}
							},
							{
								"source": {
									"path": "$['value']"
								},
								"sink": {
									"name": "value"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SRC_JsonExample3",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Dest_DB_JsonExampleParametrik",
						"type": "DatasetReference",
						"parameters": {
							"schema": "dbo",
							"table": "JsonExample3"
						}
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2022-08-05T14:26:05Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}