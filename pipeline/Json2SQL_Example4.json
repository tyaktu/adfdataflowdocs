{
	"name": "Json2SQL_Example4",
	"properties": {
		"description": "ADF içinde oluşturduğum pipieline. ",
		"activities": [
			{
				"name": "DataLakeJsonExample3toPoolSQLTable",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"writeBehavior": "Insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['type']"
								},
								"sink": {
									"name": "type"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['ppu']"
								},
								"sink": {
									"name": "ppu"
								}
							},
							{
								"source": {
									"path": "$['batters']['batter'][0]['id']"
								},
								"sink": {
									"name": "batters_id"
								}
							},
							{
								"source": {
									"path": "$['batters']['batter'][0]['type']"
								},
								"sink": {
									"name": "batters_type"
								}
							},
							{
								"source": {
									"path": "$['topping'][0]['id']"
								},
								"sink": {
									"name": "topping_id"
								}
							},
							{
								"source": {
									"path": "$['topping'][0]['type']"
								},
								"sink": {
									"name": "topping_type"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "SRC_JsonExample4",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Dest_DB_JsonExample3",
						"type": "DatasetReference",
						"parameters": {
							"schema": "dbo",
							"table": "JsonExample4"
						}
					}
				]
			}
		],
